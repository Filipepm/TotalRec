<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".nonSubTeamHome">

    <TextView
        android:id="@+id/totMemIn2"
        android:layout_width="211dp"
        android:layout_height="23dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="19dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sportIn2" />

    <TextView
        android:id="@+id/totMembersView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="71dp"
        android:text="Total Members"
        app:layout_constraintEnd_toStartOf="@+id/totMemIn2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sportView2" />

    <TextView
        android:id="@+id/teamNameIn2"
        android:layout_width="221dp"
        android:layout_height="53dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/coachNameView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="62dp"
        android:layout_marginEnd="77dp"
        android:text="Coach Name"
        app:layout_constraintEnd_toStartOf="@+id/coachNameIn2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teamNameView2" />

    <TextView
        android:id="@+id/coachNameIn2"
        android:layout_width="219dp"
        android:layout_height="24dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="17dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teamNameIn2" />

    <TextView
        android:id="@+id/sportIn2"
        android:layout_width="211dp"
        android:layout_height="25dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/creationDateIn2" />

    <TextView
        android:id="@+id/dateCreationView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="70dp"
        android:text="Creation Date:"
        app:layout_constraintEnd_toStartOf="@+id/creationDateIn2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/coachNameView2" />

    <TextView
        android:id="@+id/sportView2"
        android:layout_width="102dp"
        android:layout_height="21dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="61dp"
        android:text="Sport"
        app:layout_constraintEnd_toStartOf="@+id/sportIn2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dateCreationView2" />

    <TextView
        android:id="@+id/teamNameView2"
        android:layout_width="121dp"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="38dp"
        android:text="Team Name: "
        app:layout_constraintEnd_toStartOf="@+id/teamNameIn2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/creationDateIn2"
        android:layout_width="217dp"
        android:layout_height="27dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/coachNameIn2" />

    <Button
        android:id="@+id/joinTeamButton"
        android:layout_width="173dp"
        android:layout_height="49dp"
        android:layout_marginTop="86dp"
        android:layout_marginEnd="128dp"
        android:layout_marginBottom="256dp"
        android:text="Join the Team!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/totMemIn2" />
</androidx.constraintlayout.widget.ConstraintLayout>